,8BAAQ,SAAS,CAAEP,UAAU,CAAC,oBAAD,CAAuBK,MAAM,CAACY,UAA9B,CAA7B,eACE,2BAAK,SAAS,CAAC,WAAf,eACE,0BAAI,SAAS,CAAC,aAAd,EAA6BD,UAAU,CAACT,KAAxC,CADF,cAEE,yBAAG,SAAS,CAAC,gBAAb,EAA+BS,UAAU,CAACE,OAA1C,CAFF,cAGE,2BAAK,SAAS,CAAEb,MAAM,CAACc,OAAvB,eACE,oBAAC,IAAD,EACE,SAAS,CAAEnB,UAAU,CACnB,qDADmB,CAEnBK,MAAM,CAACe,UAFY,CADvB,CAKE,EAAE,CAAEhB,UAAU,CAAC,OAAD,CALhB,gBADF,CAHF,CADF,CAHF,cAmBE,gCACGE,QAAQ,EAAIA,QAAQ,CAACe,MAAT,CAAkB,CAA9B,eACC,+BAAS,SAAS,CAAEhB,MAAM,CAACC,QAA3B,eACE,2BAAK,SAAS,CAAC,WAAf,eACE,2BAAK,SAAS,CAAC,KAAf,EACGA,QAAQ,CAACgB,GAAT,CAAa,CAACC,KAAD,CAAQC,GAAR,gBACZ,oBAAC,OAAD,WAAS,GAAG,CAAEA,GAAd,EAAuBD,KAAvB,EADD,CADH,CADF,CADF,CAFJ,CAnBF,CADF,CAmCD,CAED,cAAeT,CAAAA,IAAf","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport Layout from '@theme/Layout';\nimport Link from '@docusaurus/Link';\nimport useDocusaurusContext from '@docusaurus/useDocusaurusContext';\nimport useBaseUrl from '@docusaurus/useBaseUrl';\nimport styles from './styles.module.css';\n\nconst features = [\n  {\n    title: <>Save Time</>,\n    // imageUrl: 'img/undraw_docusaurus_mountain.svg',\n    description: (\n      <>\n        WordWeaver was designed to be easily modified and used in a wide variety of language conjugation contexts.\n        Have a look at the <a href='docs/features'>features</a> to see if it's right for your project.\n      </>\n    ),\n  },\n  {\n    title: <>Focus on What Matters</>,\n    // imageUrl: 'img/undraw_docusaurus_tree.svg',\n    description: (\n      <>\n        WordWeaver lets you focus on your language model, and we&apos;ll do the chores and visualization. Go\n        ahead and have a read about how to <a href='docs/firststeps'>get started</a>.\n      </>\n    ),\n  },\n  {\n    title: <>Build beautiful, interactive conjugators</>,\n    // imageUrl: 'img/undraw_docusaurus_react.svg',\n    description: (\n      <>\n        Turn your language data into an interactive web and mobile conjugation application.\n      </>\n    ),\n  },\n];\n\nfunction Feature({ imageUrl, title, description }) {\n  const imgUrl = useBaseUrl(imageUrl);\n  return (\n    <div className={classnames('col col--4', styles.feature)}>\n      {imgUrl && (\n        <div className=\"text--center\">\n          <img className={styles.featureImage} src={imgUrl} alt={title} />\n        </div>\n      )}\n      <h3>{title}</h3>\n      <p>{description}</p>\n    </div>\n  );\n}\n\nfunction Home() {\n  const context = useDocusaurusContext();\n  const { siteConfig = {} } = context;\n  return (\n    <Layout\n      title={`Hello from ${siteConfig.title}`}\n      description=\"Description will go into a meta tag in <head />\">\n      <header className={classnames('hero hero--primary', styles.heroBanner)}>\n        <div className=\"container\">\n          <h1 className=\"hero__title\">{siteConfig.title}</h1>\n          <p className=\"hero__subtitle\">{siteConfig.tagline}</p>\n          <div className={styles.buttons}>\n            <Link\n              className={classnames(\n                'button button--outline button--secondary button--lg',\n                styles.getStarted,\n              )}\n              to={useBaseUrl('docs/')}>\n              Get Started\n            </Link>\n          </div>\n        </div>\n      </header>\n      <main>\n        {features && features.length > 0 && (\n          <section className={styles.features}>\n            <div className=\"container\">\n              <div className=\"row\">\n                {features.map((props, idx) => (\n                  <Feature key={idx} {...props} />\n                ))}\n              </div>\n            </div>\n          </section>\n        )}\n      </main>\n    </Layout>\n  );\n}\n\nexport default Home;\n"]}]}